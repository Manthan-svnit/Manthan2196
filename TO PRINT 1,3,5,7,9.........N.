#include <stdio.h>

int main() {
    int n, i;

    // Input the value of N from the user
    printf("Enter the value of N: ");
    scanf("%d", &n);

    // Print odd numbers from 1 to N
    printf("Odd numbers from 1 to %d: ", n);
    for (i = 1; i <= n; i += 2) {
        printf("%d ", i);
    }

    printf("\n");
    return 0;
}
