#include <stdio.h>

int main() {
    int n, i;

    // Input the value of N from the user
    printf("Enter the value of N: ");
    scanf("%d", &n);

    // Print even numbers from 2 to N
    printf("Even numbers from 2 to %d: ", n);
    for (i = 2; i <= n; i += 2) {
        printf("%d ", i);
    }

    printf("\n");
    return 0;
}
